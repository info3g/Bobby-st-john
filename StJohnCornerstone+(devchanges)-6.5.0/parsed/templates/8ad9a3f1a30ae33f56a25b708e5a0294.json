{"components/common/footer":"{{#if banners.bottom}}\r\n    {{#if settings.data_tag_enabled}}\r\n        <div class=\"banners\" data-banner-location=\"bottom\">\r\n            {{#each (limit banners.bottom_metadata 1)}}\r\n                <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\r\n                    <div data-event-type=\"promotion-click\">\r\n                        {{{this.content}}}\r\n                    </div>\r\n                </div>\r\n            {{/each}}\r\n        </div>\r\n    {{else}}\r\n        <div class=\"banners\" data-banner-location=\"bottom\">\r\n            <div class=\"banner\">\r\n                {{{limit banners.bottom 1}}}\r\n            </div>\r\n        </div>\r\n    {{/if}}\r\n\r\n{{/if}}\r\n<footer class=\"footer\" role=\"contentinfo\">\r\n    <h2 class=\"footer-title-sr-only\">{{lang 'footer.title'}}</h2>\r\n    <div class=\"container\">\r\n        {{#if theme_settings.social_icon_placement_bottom '!==' 'bottom_none'}}\r\n            <article class=\"footer-info-col\r\n                footer-info-col--social\r\n                footer-info-col--{{#if theme_settings.social_icon_placement_bottom '===' 'bottom_left'}}left{{else}}right{{/if}}\"\r\n                data-section-type=\"footer-webPages\">\r\n                    <h3 class=\"footer-info-heading\">{{lang 'social.connect'}}</h3>\r\n                    {{> components/common/social-links}}\r\n            </article>\r\n        {{/if}}\r\n        <section class=\"footer-info\">\r\n            <article class=\"footer-info-col footer-info-col--small footer-article-custom\" data-section-type=\"footer-webPages\">\r\n                <div class=\"footer-info-list footer-logo\">\r\n                    <div class=\"footer-logo-image\">\r\n                        <img src=\"https://cdn11.bigcommerce.com/s-hw9fq6q6i6/images/stencil/250x100/image_1_1661585390__10156.original.jpg\" alt=\"\" />\r\n                    </div>\r\n                    <div>\r\n                        <h5>St John Ambulance Australia NSW</h5>\r\n                        <p>1300 St John (78 5646)</p>\r\n                    </div>\r\n                </div>\r\n            </article>\r\n\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-categories\">\r\n                <div class=\"footer-info-list footer-logo\">\r\n                    <div class=\"footer-logo-image\">\r\n                        <img src=\"https://i.shgcdn.com/d3535f52-bb22-4baa-ba7b-d5cd4a5df35b/-/format/auto/-/preview/3000x3000/-/quality/lighter/\" alt=\"\" />\r\n                    </div>\r\n                    <div class=\"footer-logo-image\">\r\n                        <img src=\"https://i.shgcdn.com/122ec5cc-76a5-4b63-8cab-f1245e20f6c7/-/format/auto/-/preview/3000x3000/-/quality/lighter/\" alt=\"\" />\r\n                    </div>\r\n                    <div class=\"footer-logo-image\">\r\n                        <img src=\"https://i.shgcdn.com/5686b4bd-5113-4a67-a9d3-04fde048ff89/-/format/auto/-/preview/3000x3000/-/quality/lighter/\" alt=\"\" />\r\n                    </div>\r\n                </div>\r\n            </article>\r\n\r\n            <article class=\"footer-info-col\" data-section-type=\"newsletterSubscription\">\r\n                {{#if settings.show_newsletter_box}}\r\n                    {{> components/common/subscription-form}}\r\n                {{/if}}\r\n                {{> components/common/payment-icons}}\r\n            </article>\r\n        </section>\r\n        <section class=\"footer-info\">\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\r\n                <ul class=\"footer-info-list\">\r\n                    <li class=\"margin-style\">\r\n                        <a href=\"/businesses-events-main-page\">For business</a>\r\n       